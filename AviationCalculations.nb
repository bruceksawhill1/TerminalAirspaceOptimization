Cell["\<\
The approximation to second order for the cosine of the arctan of v^2/gR (the \
bank angle) is:\
\>", "Text",
 CellChangeTimes->{{3.573067519940875*^9, 3.57306752442832*^9}, {
   3.573073131210202*^9, 3.573073220563857*^9}, {3.573171305455758*^9, 
   3.573171329914104*^9}, {3.5731717849858723`*^9, 3.573171855981833*^9}, {
   3.57317192746917*^9, 3.573171965371111*^9}, {3.5731719979556313`*^9, 
   3.573172023023937*^9}, 3.573172614671397*^9, {3.573172743634925*^9, 
   3.57317278080663*^9}, 3.57317348143233*^9, {3.5731735692579927`*^9, 
   3.573173569596281*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"cphi", "[", 
   RowBox[{"vx_", ",", "vy_", ",", "ax_", ",", "ay_"}], "]"}], ":=", 
  RowBox[{"Series", "[", 
   RowBox[{
    RowBox[{"Cos", "[", 
     RowBox[{"ArcTan", "[", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"vx", "^", "2"}], "+", 
         RowBox[{"vy", "^", "2"}]}], ")"}], "/", 
       RowBox[{"(", 
        RowBox[{"9.80665", " ", 
         RowBox[{"rxy", "[", 
          RowBox[{"vx", ",", "vy", ",", "ax", ",", "ay"}], "]"}]}], ")"}]}], 
      "]"}], "]"}], ",", " ", 
    RowBox[{"{", 
     RowBox[{"x", ",", "0", ",", "2"}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.5731714860521793`*^9, 3.573171539929199*^9}, {
  3.573171581019463*^9, 3.573171655094767*^9}, {3.573171878807313*^9, 
  3.573171898807364*^9}}],

