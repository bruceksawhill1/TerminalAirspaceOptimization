   3.5730682849227257`*^9, 3.5730682856821947`*^9}, 3.573068328093553*^9, {
   3.573070182612856*^9, 3.573070198585939*^9}, {3.5730702445266037`*^9, 
   3.5730703026985826`*^9}, {3.573070504887895*^9, 3.573070533461856*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"maxclimbthrust", "[", 
  RowBox[{"146590", ",", "53872", ",", 
   RowBox[{"0.30453", " ", 
    RowBox[{"10", "^", 
     RowBox[{"(", 
      RowBox[{"-", "10"}], ")"}]}]}], ",", "h"}], "]"}]], "Input",
